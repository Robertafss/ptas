<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' type='text/css' media='screen' href='estilo/estilo.css'/>

    <title>Cadastrar</title>
</head>
<body>
    <div id='cadas'>
    <a href='/pessoas/criar'> Cadastrar</a>
<div id='cadtrar'> 
    <% pessoas.forEach((pessoa) => { %>
        <div>   <%= pessoa.nome %> </div>
        <div><%= pessoa.id %> </div>
   
        <button> <a href="/pessoas/deletar?id= <%= pessoa.id %>" >  Excluir </a></button>
       <% }) %>
</div> 
  

</div>

</div>
</body>
</html>